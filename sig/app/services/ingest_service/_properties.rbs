# IngestService::Properties
#
module IngestService::Properties
  include ApiService::Properties

  # Configuration values from config/locales/service.en.yml
  #
  # @type [Hash{Symbol=>Any}]
  #
  CONFIGURATION: symHash

  DEFAULT_ENGINE: Symbol

  def configuration: () -> symHash

  # The URL for the API connection.
  #
  # @return [String]
  #
  # @see #default_engine_url
  #
  #def base_url: () -> String

  # Federated Ingest API key.
  #
  # @return [String, nil]
  #
  # @see #INGEST_API_KEY
  #
  def api_key: () -> (String | nil)

  # API version is not a part of request URLs.
  #
  # @return [nil]
  #
  #def api_version: () -> nil

  # Valid search engine URLs.
  #
  # @type [Hash{Symbol=>String}]
  #
  #def engines: () -> Hash[Symbol,String]

  # Default search engine for this deployment.
  #
  # @return [String]
  #
  def default_engine_url: () -> String

  # The default service engine key.
  #
  # @return [Symbol]
  #
  def default_engine_key: () -> Symbol

  private

  def self.included: (Module base) -> void
end
