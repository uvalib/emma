<%# app/views/sys/_menu.html.erb -%>
<%-
  # System information pages menu.

  view_debug(__FILE__)

  level ||= 3

  outer = (level < 3) ? :div : :ul
  inner = (level < 3) ? :div : :li
  links = action_links(tag: inner)

-%>

<%= html_tag(outer, links, class: 'page-actions', role: 'navigation') %>
