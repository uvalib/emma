<%# app/views/sys/_command.html.erb -%>
<%-
  # Display shell output.

  view_debug(__FILE__)

  command ||= nil
  lines   ||= run_command(command)

  view_abort(__FILE__, ':command not passed via "locals"') unless lines

-%>

<div class="output" data-turbolinks-permanent="true"><%= h(lines) -%></div>
