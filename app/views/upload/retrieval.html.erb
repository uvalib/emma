<%# app/views/upload/retrieval.html.erb -%>
<%-
  # Fall-back page for a failed Upload retrieval action.

  view_debug(__FILE__)

  cfg   ||= config_page_section(:download)
  title ||= cfg[:title] || cfg[:label]
  error ||= cfg.dig(:error, :server)

-%>
<%= render 'upload/fallback', title: title, error: error %>
