<%# app/views/upload/bulk_reindex.html.erb -%>
<%-
  # Results of re-indexing.
  title ||= t('emma.upload.bulk_reindex.title')
  list  ||= @list || []
-%>

<%= render 'fallback', title: title, error: 'Items:' %>

<ul>
  <%- list.each do |item| -%>
    <li><%= Upload.sid_for(item) -%></li>
  <%- end -%>
</ul>
