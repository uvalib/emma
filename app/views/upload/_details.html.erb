<%# app/views/upload/_details.html.erb -%>
<%-
  # A listing of an upload entry details.

  view_debug(__FILE__)

  item  ||= nil
  image ||= nil
  dec   ||= item && UploadDecorator.new(item, cover: image, **context) or return

  level ||= 1
  extra ||= nil
  skip  ||= nil

-%>

<%= dec.details_container(level: level, pairs: extra, skip: skip) %>
