<%# app/views/manifest_item/retrieval.html.erb -%>
<%-
  # Fall-back page for a failed ManifestItem retrieval action.

  view_abort(__FILE__, 'NOT EXPECTED TO BE USED')
  view_debug(__FILE__)

  cfg   ||= current_config_page_section
  title ||= cfg[:title] || cfg[:label]
  error ||= cfg.dig(:error, :server)

-%>
<%= render 'manifest_item/fallback', title: title, error: error %>
