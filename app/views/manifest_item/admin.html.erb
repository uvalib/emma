<%# app/views/manifest_item/admin.html.erb -%>
<%-
  # ManifestItem submission administration.

  view_abort(__FILE__, 'NOT EXPECTED TO BE USED')
  view_debug(__FILE__)

  help  ||= nil # %i[manifest_item admin]
  title ||= t('emma.manifest_item.admin.title')

  unless modal?
    set_page_meta_title(title)
    set_page_description(title)
  end

-%>

